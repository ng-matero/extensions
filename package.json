{
  "name": "extensions-srcs",
  "version": "0.0.0-NOT-USED",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve:dev-app",
    "build": "ng build",
    "test": "ng test",
    "e2e": "ng e2e",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:ts": "eslint \"projects/**/*.ts\" --fix",
    "lint:scss": "stylelint \"projects/**/*.scss\" --fix",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "serve:dev-app": "ng serve --project=dev-app",
    "serve:docs": "ng serve --project=docs",
    "build:docs": "ng build docs --baseHref=/extensions/",
    "predeploy": "npm run build:docs",
    "deploy": "angular-cli-ghpages --no-silent --dir=dist/docs",
    "build:extensions": "ng build extensions --configuration production",
    "build:extensions-moment-adapter": "ng build extensions-moment-adapter --configuration production",
    "build:lib": "npm run build:extensions",
    "publish": "npm run build:lib && cd dist/extensions && npm publish",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.6",
    "@angular/cdk": "~13.3.6",
    "@angular/common": "~13.3.6",
    "@angular/compiler": "~13.3.6",
    "@angular/core": "~13.3.6",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "~13.3.6",
    "@angular/localize": "~13.3.6",
    "@angular/material": "~13.3.6",
    "@angular/material-moment-adapter": "~13.3.6",
    "@angular/platform-browser": "~13.3.6",
    "@angular/platform-browser-dynamic": "~13.3.6",
    "@angular/router": "~13.3.6",
    "@ng-select/ng-option-highlight": "0.0.7",
    "@ng-select/ng-select": "^8.1.1",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "moment": "^2.29.3",
    "ngx-color": "^7.3.3",
    "ngx-progressbar": "^8.0.0",
    "photoviewer": "^3.6.2",
    "rxjs": "~7.5.5",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.5",
    "@angular-eslint/builder": "^13.2.0",
    "@angular-eslint/eslint-plugin": "^13.2.0",
    "@angular-eslint/eslint-plugin-template": "^13.2.0",
    "@angular-eslint/schematics": "^13.2.0",
    "@angular-eslint/template-parser": "^13.2.0",
    "@angular/cli": "~13.3.5",
    "@angular/compiler-cli": "~13.3.6",
    "@angular/language-service": "~13.3.6",
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@types/jasmine": "~3.8.2",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "angular-cli-ghpages": "^1.0.0",
    "eslint": "^8.15.0",
    "gulp": "^4.0.2",
    "highlight-loader": "^0.7.4",
    "highlight.js": "^11.5.0",
    "husky": "^7.0.4",
    "jasmine-core": "~4.0.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.19",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lint-staged": "^12.4.1",
    "markdown-loader": "^8.0.0",
    "ng-packagr": "^13.3.1",
    "prettier": "^2.6.2",
    "protractor": "~7.0.0",
    "raw-loader": "^4.0.2",
    "stylelint": "^14.8.0",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-scss": "^4.2.0",
    "ts-node": "~10.7.0",
    "tsickle": "^0.46.0",
    "typescript": "~4.6.4"
  }
}

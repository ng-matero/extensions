{
  "name": "extensions-srcs",
  "version": "0.0.0-NOT-USED",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve:dev-app",
    "build": "ng build",
    "test": "ng test",
    "e2e": "ng e2e",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:ts": "eslint \"projects/**/*.ts\" --fix",
    "lint:scss": "stylelint \"projects/**/*.scss\" --fix",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "serve:dev-app": "ng serve --project=dev-app",
    "serve:docs": "ng serve --project=docs",
    "build:docs": "ng build docs --base-href=/extensions/",
    "predeploy": "npm run build:docs",
    "deploy": "angular-cli-ghpages --no-silent --dir=dist/docs",
    "build:lib": "ng build extensions --configuration production && npm run build:themes",
    "build:themes": "sass projects/extensions/core/theming/prebuilt:dist/extensions/prebuilt-themes --load-path=node_modules --style=compressed --no-source-map",
    "build:moment-adapter": "ng build extensions-moment-adapter --configuration production",
    "build:luxon-adapter": "ng build extensions-luxon-adapter --configuration production",
    "build:date-fns-adapter": "ng build extensions-date-fns-adapter --configuration production",
    "publish": "npm run build:lib && cd dist/extensions && npm publish",
    "publish:moment-adapter": "npm run build:moment-adapter && cd dist/extensions-moment-adapter && npm publish",
    "publish:luxon-adapter": "npm run build:luxon-adapter && cd dist/extensions-luxon-adapter && npm publish",
    "publish:date-fns-adapter": "npm run build:date-fns-adapter && cd dist/extensions-date-fns-adapter && npm publish",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.1",
    "@angular/cdk": "^15.2.1",
    "@angular/common": "^15.2.1",
    "@angular/compiler": "^15.2.1",
    "@angular/core": "^15.2.1",
    "@angular/forms": "^15.2.1",
    "@angular/localize": "^15.2.1",
    "@angular/material": "^15.2.1",
    "@angular/material-date-fns-adapter": "^15.2.1",
    "@angular/material-luxon-adapter": "^15.2.1",
    "@angular/material-moment-adapter": "^15.2.1",
    "@angular/platform-browser": "^15.2.1",
    "@angular/platform-browser-dynamic": "^15.2.1",
    "@angular/router": "^15.2.1",
    "@ng-select/ng-option-highlight": "0.0.7",
    "@ng-select/ng-select": "^10.0.3",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "date-fns": "^2.29.3",
    "luxon": "^3.2.1",
    "moment": "^2.29.4",
    "ngx-color": "^8.0.3",
    "ngx-progressbar": "^9.0.0",
    "photoviewer": "^3.6.6",
    "rxjs": "~7.5.6",
    "tslib": "^2.4.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.1",
    "@angular-eslint/builder": "^15.2.0",
    "@angular-eslint/eslint-plugin": "^15.2.0",
    "@angular-eslint/eslint-plugin-template": "^15.2.0",
    "@angular-eslint/schematics": "^15.2.0",
    "@angular-eslint/template-parser": "^15.2.0",
    "@angular/cli": "^15.2.1",
    "@angular/compiler-cli": "^15.2.1",
    "@angular/language-service": "^15.2.1",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@types/jasmine": "~4.3.0",
    "@types/jasminewd2": "~2.0.10",
    "@types/luxon": "^3.2.0",
    "@types/node": "^18.11.0",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "angular-cli-ghpages": "^1.0.5",
    "eslint": "^8.32.0",
    "gulp": "^4.0.2",
    "highlight-loader": "^0.7.4",
    "highlight.js": "^11.7.0",
    "husky": "^8.0.3",
    "jasmine-core": "~4.5.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lint-staged": "^13.1.0",
    "markdown-loader": "^8.0.0",
    "ng-packagr": "^15.1.1",
    "prettier": "^2.8.3",
    "protractor": "~7.0.0",
    "raw-loader": "^4.0.2",
    "stylelint": "^14.16.0",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-scss": "^4.3.0",
    "ts-node": "~10.9.0",
    "tsickle": "^0.46.0",
    "typescript": "~4.8.4"
  }
}
{
  "name": "extensions-srcs",
  "version": "0.0.0-NOT-USED",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve:dev-app",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "serve:dev-app": "ng serve --project=dev-app",
    "serve:docs": "ng serve --project=docs",
    "build:docs": "ng build docs --prod --baseHref=/extensions/",
    "deploy:docs": "angular-cli-ghpages --no-silent --dir=dist/docs",
    "release": "npm run build:docs && npm run deploy:docs",
    "build:lib": "ng build extensions --prod",
    "copy:scss": "gulp --gulpfile gulpfile.js",
    "publish": "npm run build:lib && npm run copy:scss && cd dist/extensions && npm publish --access public"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.1.0",
    "@angular/cdk": "~10.2.7",
    "@angular/common": "~11.1.0",
    "@angular/compiler": "~11.1.0",
    "@angular/core": "~11.1.0",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "~11.1.0",
    "@angular/localize": "~11.1.0",
    "@angular/material": "~10.2.7",
    "@angular/platform-browser": "~11.1.0",
    "@angular/platform-browser-dynamic": "~11.1.0",
    "@angular/router": "~11.1.0",
    "@ng-select/ng-option-highlight": "0.0.6",
    "@ng-select/ng-select": "^5.1.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "ngx-color": "^6.1.0",
    "ngx-progressbar": "^6.0.3",
    "photoviewer": "^3.5.4",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.1",
    "@angular-devkit/build-ng-packagr": "~0.1002.1",
    "@angular/cli": "~10.2.1",
    "@angular/compiler-cli": "~11.1.0",
    "@angular/language-service": "~11.1.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^0.6.2",
    "codelyzer": "^6.0.0",
    "gulp": "^4.0.2",
    "highlight-loader": "^0.7.4",
    "highlight.js": "^10.2.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "markdown-loader": "^6.0.0",
    "ng-packagr": "^10.1.2",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "raw-loader": "^4.0.2",
    "stylelint": "^13.8.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "ts-node": "~8.3.0",
    "tsickle": "^0.39.1",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}

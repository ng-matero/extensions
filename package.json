{
  "name": "extensions-srcs",
  "version": "0.0.0-NOT-USED",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve:dev-app",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "serve:dev-app": "ng serve --project=dev-app",
    "serve:docs": "ng serve --project=docs",
    "build:docs": "ng build docs --configuration production --baseHref=/extensions/",
    "deploy:docs": "angular-cli-ghpages --no-silent --dir=dist/docs",
    "release": "npm run build:docs && npm run deploy:docs",
    "build:lib": "ng build extensions --configuration production",
    "copy:scss": "gulp --gulpfile gulpfile.js",
    "publish": "npm run build:lib && npm run copy:scss && cd dist/extensions && npm publish --access public"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.1.2",
    "@angular/cdk": "~12.1.1",
    "@angular/common": "~12.1.2",
    "@angular/compiler": "~12.1.2",
    "@angular/core": "~12.1.2",
    "@angular/flex-layout": "^12.0.0-beta.34",
    "@angular/forms": "~12.1.2",
    "@angular/localize": "~12.1.2",
    "@angular/material": "~12.1.1",
    "@angular/platform-browser": "~12.1.1",
    "@angular/platform-browser-dynamic": "~12.1.2",
    "@angular/router": "~12.1.2",
    "@ng-select/ng-option-highlight": "0.0.6",
    "@ng-select/ng-select": "^7.1.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "ngx-color": "^7.2.0",
    "ngx-progressbar": "^6.1.1",
    "photoviewer": "^3.5.8",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.1",
    "@angular/cli": "~12.1.1",
    "@angular/compiler-cli": "~12.1.2",
    "@angular/language-service": "~12.1.2",
    "@types/jasmine": "~3.7.7",
    "@types/jasminewd2": "~2.0.9",
    "@types/node": "^16.0.0",
    "angular-cli-ghpages": "^1.0.0-rc.2",
    "codelyzer": "^6.0.2",
    "gulp": "^4.0.2",
    "highlight-loader": "^0.7.4",
    "highlight.js": "^10.7.3",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.6.0",
    "markdown-loader": "^6.0.0",
    "ng-packagr": "^12.1.0",
    "prettier": "^2.3.2",
    "protractor": "~7.0.0",
    "raw-loader": "^4.0.2",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.19.0",
    "ts-node": "~10.0.0",
    "tsickle": "^0.43.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}

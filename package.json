{
  "name": "extensions-srcs",
  "version": "0.0.0-NOT-USED",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve:dev-app",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "serve:dev-app": "ng serve --project=dev-app",
    "serve:docs": "ng serve --project=docs",
    "build:docs": "ng build docs --prod --baseHref=/extensions/",
    "deploy:docs": "angular-cli-ghpages --no-silent --dir=dist/docs",
    "release": "npm run build:docs && npm run deploy:docs",
    "build:lib": "ng build extensions --prod",
    "copy:scss": "gulp --gulpfile gulpfile.js",
    "publish": "npm run build:lib && npm run copy:scss && cd dist/extensions && npm publish --access public"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.13",
    "@angular/cdk": "~11.2.12",
    "@angular/common": "~11.2.13",
    "@angular/compiler": "~11.2.13",
    "@angular/core": "~11.2.13",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~11.2.13",
    "@angular/localize": "~11.2.13",
    "@angular/material": "~11.2.12",
    "@angular/platform-browser": "~11.2.13",
    "@angular/platform-browser-dynamic": "~11.2.13",
    "@angular/router": "~11.2.13",
    "@ng-select/ng-option-highlight": "0.0.6",
    "@ng-select/ng-select": "^6.1.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "ngx-color": "^6.2.0",
    "ngx-progressbar": "^6.1.0",
    "photoviewer": "^3.5.8",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.12",
    "@angular/cli": "~11.2.12",
    "@angular/compiler-cli": "~11.2.13",
    "@angular/language-service": "~11.2.13",
    "@types/jasmine": "~3.7.4",
    "@types/jasminewd2": "~2.0.9",
    "@types/node": "^15.0.2",
    "angular-cli-ghpages": "^1.0.0-rc.1",
    "codelyzer": "^6.0.2",
    "gulp": "^4.0.2",
    "highlight-loader": "^0.7.4",
    "highlight.js": "^10.7.2",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.6.0",
    "markdown-loader": "^6.0.0",
    "ng-packagr": "^11.2.4",
    "prettier": "^2.3.0",
    "protractor": "~7.0.0",
    "raw-loader": "^4.0.2",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.19.0",
    "ts-node": "~9.1.1",
    "tsickle": "^0.39.1",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  }
}
